{
  "manifest_version": 2,
  "name": "SCleaner",
  "version": "1.0",
  "description": "Clean shits from tiny zone",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": ["https://*/*", "http://*/*", "tabs", "activeTab"]
}
